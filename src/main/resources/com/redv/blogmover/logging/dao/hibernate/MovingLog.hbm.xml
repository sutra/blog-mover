<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.redv.blogmover.logging">
	<class name="MovingLog" table="movinglog">
		<id name="id" unsaved-value="null" type="java.lang.String"
			length="32">
			<generator class="uuid" />
		</id>
		<property name="from" column="fromEntry" insert="true" />
		<property name="to" column="toEntry" insert="true" />
		<many-to-one name="moving" class="Moving" column="movingId" />
	</class>
</hibernate-mapping>
